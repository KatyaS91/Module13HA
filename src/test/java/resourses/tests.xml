<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Tests" verbose="1">
    <listeners>
        <listener class-name="utils.screenshotlistener.ScreenshotListener" />
    </listeners>
    <test name="Tests">
        <parameter name="browser" value="Chrome" />
        <groups>
            <run>
                <exclude name="p1"></exclude>
            </run>
        </groups>
        <packages>
            <package name="tests.basetest"></package>
        </packages>
    </test>
</suite>